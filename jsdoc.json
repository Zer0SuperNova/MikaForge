{
  "source": {
    "include": ["Src", "main.js", "Packer"],
    "exclude": ["node_modules", "test"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme"
  }
}
